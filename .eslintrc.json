{
    // env : 프로젝트 환경 설정
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    // extends : ESLint 확장
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
    "ecmaVersion": 12,
    "parser": "@typescript-eslint/parser"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    // rules : 프로젝트 내에서 강제할 규칙 정의
    "rules": {
    }
}
